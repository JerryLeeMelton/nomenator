{
  "name": "nomenator",
  "version": "1.0.0",
  "description": "Nomenator is a GUI tool for naming files.",
  "main": "dist/main/main.js",
  "license": "MIT",
  "scripts": {
    "dev:renderer": "vite",
    "dev:main": "tsc -w",
    "dev:electron": "wait-on http://localhost:5173 && electron dist/main/main.js",
